<h1>Letters#new</h1>
<p>Find me in app/views/letters/new.html.erb</p>

<%= form_for :letter, url: letters_path do |f| %>
  <p>
      <%= f.label :title %><br>
      <%= f.text_field :title %>
  </p>
 
  <p>
      <%= f.label :text %><br>
      <%= f.text_area :text %>
  </p>
   
  <p>
      <%= f.submit %>
  </p>
<% end %>
